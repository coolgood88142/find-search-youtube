{
  "name": "Search Youtube Video",
  "version": "1.2",
  "description": "Quickly Search Youtube ",
  "permissions": [
    "contextMenus",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/image16.png",
      "32": "images/image32.png",
      "48": "images/image48.png",
      "128": "images/image128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/image16.png",
    "32": "images/image32.png",
    "48": "images/image48.png",
    "128": "images/image128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/axios.min.js",
        "js/content.js"
      ]
    }
  ],
  "manifest_version": 3
}